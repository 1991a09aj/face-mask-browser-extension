{
  "extends": ["onepass"],
  "rules": {
    "no-console": 0,
    "no-extend-native": 0,
    "jsx-a11y/label-has-associated-control": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx"],
        "moduleDirectory": ["node_modules", "src/"]
      }
    }
  }
}
